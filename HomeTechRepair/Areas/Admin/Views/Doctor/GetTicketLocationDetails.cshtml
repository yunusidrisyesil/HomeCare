@model SupportTicket
@{
    ViewBag.Title = "Location";
}
@section Styles{
<link rel="stylesheet" href="~/assets/css/maps.css">
}

<div id="map">
</div>

@section Scripts{
<script src=~/assets/js/maps.js></script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD1zFGX6A3lCgEI_JvZfbl4F8Zsx0XPOaU&callback=initMap" async defer>
</script>

<script>
        $(() => {
            function delay(time) {
              return new Promise(resolve => setTimeout(resolve, time));
            }
            var location = { lat: @Model.Latitude, lng: @Model.Longitude };
            delay(1000).then(() =>setMarker(location));
            //centerMap(location);
        });
</script>
}


